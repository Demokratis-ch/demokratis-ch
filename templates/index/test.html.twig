{% extends 'base.html.twig' %}

{% block title %}Demokratis.ch{% endblock %}

{% block htmlbody %}
    <div class="bg-white">
        <main>
            <!-- Hero section -->
            <div class="overflow-hidden pt-8 sm:pt-12 lg:relative lg:py-48">
                <div class="mx-auto max-w-md px-6 sm:max-w-3xl lg:grid lg:max-w-7xl lg:grid-cols-2 lg:gap-24 lg:px-8">
                    <div>
                        <div class="">
                            <div>
                                <div class="inline-flex space-x-4">
                                    <span class="rounded bg-gray-100 px-2.5 py-1 text-sm font-semibold text-gray-500">Alpha</span>
                                    <a href="#" class="inline-flex items-center space-x-1 text-sm font-medium text-gray-500 hover:underline">
                                        <span>Was ist eine Vernehmlassung?</span>
                                        <i class="fas fa-chevron-right text-xs"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="mt-3 sm:max-w-xl">
                                <h1 class="text-4xl font-bold tracking-tight text-gray-900 sm:text-5xl">Die Plattform für <a href="{{ path('app_consultation') }}" class="text-blue-600">eidgenössische Vernehmlassungen</a></h1>
                                <p class="mt-6 text-xl text-gray-500">Wir denken das Vernehmlassungsverfahren neu: <br class="hidden md:inline-block">Mit zeitgemässen Medien, offen für alle</p>
                            </div>

                            {% if (app.flashes | length) > 0 %}
                                {% for label, messages in app.flashes(['newsletter']) %}
                                    <div class="rounded-md bg-green-50 border border-green-200 border-solid p-4">
                                        <div class="flex">
                                            <div class="flex-shrink-0">
                                                <!-- Heroicon name: solid/check-circle -->
                                                <svg class="h-5 w-5 text-green-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                                                </svg>
                                            </div>
                                            <div class="ml-3">
                                                <p class="text-sm font-medium text-green-800">Erfolgreich eingetragen</p>
                                            </div>
                                        </div>
                                    </div>

                                {% endfor %}
                            {% else %}
                                {{ form_start(form, { 'attr' : { 'class': 'mt-12 sm:flex sm:w-full sm:max-w-lg' } }) }}
                            <div class="min-w-0 flex-1">
                                {{ form_row(form.email, { 'attr' : { 'class': 'block w-full rounded-md border border-gray-300 px-5 py-3 text-base text-gray-900 placeholder-gray-500 shadow-sm focus:border-blue-500 focus:ring-blue-500' } }) }}
                            </div>
                                <div class="mt-4 sm:mt-0 sm:ml-3">
                                    {{ form_row(form.submit, { 'attr' : { 'class': 'block w-full rounded-md border border-transparent bg-blue-500 px-5 py-3 text-base font-medium text-white shadow hover:bg-blue-600 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 sm:px-10' } }) }}
                            </div>
                                {{ form_end(form) }}
                            {% endif %}

                            <div class="py-1 text-sm text-gray-500">Aktuell laufen <a href="{{ path('app_consultation', { 'filter': 'ongoing' }) }}" class="font-medium text-blue-500 hover:underline">{{ ongoing }}</a> Vernehmlassungen und <a href="{{ path('app_consultation', { 'filter': 'planned' }) }}" class="font-medium text-blue-500 hover:underline">{{ planned }}</a> sind geplant.</div>
                        </div>
                    </div>
                </div>

                <div class="sm:mx-auto sm:max-w-3xl sm:px-6">
                    <div class="py-12 sm:relative sm:mt-12 sm:py-16 lg:absolute lg:inset-y-0 lg:right-0 lg:w-1/2">
                        <div class="hidden sm:block">
                            <div class="absolute inset-y-0 left-1/2 w-screen rounded-l-3xl bg-gray-50 lg:left-80 lg:right-0 lg:w-full"></div>
                            <svg class="absolute top-8 right-1/2 -mr-3 lg:left-0 lg:m-0" width="404" height="392" fill="none" viewBox="0 0 404 392">
                                <defs>
                                    <pattern id="837c3e70-6c3a-44e6-8854-cc48c737b659" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                                        <rect x="0" y="0" width="4" height="4" class="text-gray-200" fill="currentColor" />
                                    </pattern>
                                </defs>
                                <rect width="404" height="392" fill="url(#837c3e70-6c3a-44e6-8854-cc48c737b659)" />
                            </svg>
                        </div>
                        <div class="relative -mr-40 pl-6 sm:mx-auto sm:max-w-3xl sm:px-0 lg:h-full lg:max-w-none lg:pl-12">
                            <span class="md:ml-4 text-center text-sm font-medium text-gray-500 bg-white p-1">
                                Die neusten Vernehmlassungen
                            </span>

                            <div class="mt-1 overflow-hidden bg-white shadow sm:rounded-md">
                                <ul role="list" class="divide-y divide-gray-200">

                                    {% for consultation in consultations %}
                                    <li class="group">
                                        <a href="{{ path('app_consultation_show_statements', { 'slug': consultation.slug }) }}" class="block hover:bg-gray-50">
                                            <div class="flex items-center px-4 py-4 sm:px-6">
                                                <div class="min-w-0 flex-1 sm:flex sm:items-center sm:justify-between">
                                                    <div class="truncate">
                                                        <div class="flex text-sm">
                                                            <p class="truncate font-medium text-blue-600 group-hover:text-blue-500">{{ consultation.title }}</p>
                                                            <p class="ml-1 flex-shrink-0 font-normal text-gray-500 group-hover:text-gray-400">
                                                            </p>
                                                        </div>
                                                        <div class="mt-2 flex">
                                                            <div class="flex items-center text-sm text-gray-500 group-hover:text-gray-400">
                                                                <!-- Heroicon name: mini/calendar -->
                                                                <svg class="mr-1.5 h-5 w-5 flex-shrink-0 text-gray-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                                    <path fill-rule="evenodd" d="M5.75 2a.75.75 0 01.75.75V4h7V2.75a.75.75 0 011.5 0V4h.25A2.75 2.75 0 0118 6.75v8.5A2.75 2.75 0 0115.25 18H4.75A2.75 2.75 0 012 15.25v-8.5A2.75 2.75 0 014.75 4H5V2.75A.75.75 0 015.75 2zm-1 5.5c-.69 0-1.25.56-1.25 1.25v6.5c0 .69.56 1.25 1.25 1.25h10.5c.69 0 1.25-.56 1.25-1.25v-6.5c0-.69-.56-1.25-1.25-1.25H4.75z" clip-rule="evenodd" />
                                                                </svg>
                                                                <p>
                                                                    {% if consultation.startDate is defined and consultation.startDate is not null and consultation.endDate is defined and consultation.endDate is not null %}
                                                                        {{ consultation.startDate|date('d. m. Y')|default() }} - {{ consultation.endDate|date('d. m. Y')|default() }}
                                                                    {% endif %}
                                                                    </p>
                                                                <p class="ml-2">
                                                                    {% for tag in consultation.tags %}
                                                                        <span class="inline-block p-1 bg-gray-50 border text-gray-400 text-xs rounded-xl">{{ tag.name }}</span>
                                                                    {% endfor %}
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="ml-5 flex-shrink-0">
                                                    <!-- Heroicon name: mini/chevron-right -->
                                                    <svg class="h-5 w-5 text-gray-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                        <path fill-rule="evenodd" d="M7.21 14.77a.75.75 0 01.02-1.06L11.168 10 7.23 6.29a.75.75 0 111.04-1.08l4.5 4.25a.75.75 0 010 1.08l-4.5 4.25a.75.75 0 01-1.06-.02z" clip-rule="evenodd" />
                                                    </svg>
                                                </div>
                                            </div>
                                        </a>
                                    </li>
                                    {% endfor %}

                                </ul>
                            </div>

                            <a href="{{ path('app_consultation') }}" class="underline hover:no-underline mt-2 md:ml-4 p-1 bg-white inline-flex items-center space-x-1 text-sm font-medium text-gray-500 hover:text-gray-800 hover:underline">
                                <span class="">Liste aller {{ count }} Vernehmlassungen</span>
                                <i class="fas fa-chevron-right text-xs"></i>
                            </a>

                        </div>
                    </div>
                </div>
            </div>

            <!-- Testimonial/stats section -->
            <div class="relative mt-20">
                <div class="lg:mx-auto lg:grid lg:max-w-7xl lg:grid-cols-2 lg:items-start lg:gap-24 lg:px-8">
                    <div class="relative sm:py-16 lg:py-0">
                        <div aria-hidden="true" class="hidden sm:block lg:absolute lg:inset-y-0 lg:right-0 lg:w-screen">
                            <div class="absolute inset-y-0 right-1/2 w-full rounded-r-3xl bg-gray-50 lg:right-72"></div>
                            <svg class="absolute top-8 left-1/2 -ml-3 lg:-right-8 lg:left-auto lg:top-12" width="404" height="392" fill="none" viewBox="0 0 404 392">
                                <defs>
                                    <pattern id="02f20b47-fd69-4224-a62a-4c9de5c763f7" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                                        <rect x="0" y="0" width="4" height="4" class="text-gray-200" fill="currentColor" />
                                    </pattern>
                                </defs>
                                <rect width="404" height="392" fill="url(#02f20b47-fd69-4224-a62a-4c9de5c763f7)" />
                            </svg>
                        </div>
                        <div class="relative mx-auto max-w-md px-6 sm:max-w-3xl lg:max-w-none lg:px-0 lg:py-20">
                            <!-- Testimonial card-->
                            <div class="relative overflow-hidden rounded-2xl pt-64 pb-10 shadow-xl">
                                <img class="absolute inset-0 {# h-full #} w-full object-cover" src="https://schweizermonat.ch/wp-content/uploads/2021/08/Titel_Schafroth_ohnetext.jpg" alt="">
                                <div class="absolute inset-0 bg-gray-50 mix-blend-multiply"></div>
                                <div class="absolute inset-0 bg-gradient-to-t from-blue-600 via-gray-400 opacity-90"></div>
                                <div class="relative px-8">
                                    <blockquote class="mt-8">
                                        <div class="opacity-80">
                                            <img class="h-24" src="{{ asset('img/campus-demokratie-white.png') }}" alt="Campus für Demokratie">
                                        </div>
                                        <div class="relative text-lg font-medium text-white md:flex-grow">
                                            <svg class="absolute top-0 left-0 h-8 w-8 -translate-x-3 -translate-y-2 transform text-blue-400" fill="currentColor" viewBox="0 0 32 32" aria-hidden="true">
                                                <path d="M9.352 4C4.456 7.456 1 13.12 1 19.36c0 5.088 3.072 8.064 6.624 8.064 3.36 0 5.856-2.688 5.856-5.856 0-3.168-2.208-5.472-5.088-5.472-.576 0-1.344.096-1.536.192.48-3.264 3.552-7.104 6.624-9.024L9.352 4zm16.512 0c-4.8 3.456-8.256 9.12-8.256 15.36 0 5.088 3.072 8.064 6.624 8.064 3.264 0 5.856-2.688 5.856-5.856 0-3.168-2.304-5.472-5.184-5.472-.576 0-1.248.096-1.44.192.48-3.264 3.456-7.104 6.528-9.024L25.864 4z" />
                                            </svg>
                                            <p class="relative">Wir sind begeistert von der innovativen Idee und dem Kernanliegen einer Förderung der demokratischen Partizipation. </p>
                                        </div>

                                        <footer class="mt-4">
                                            <p class="text-base font-semibold text-blue-200">Carol Scharfoth, CEO Campus für Demokratie</p>
                                        </footer>
                                    </blockquote>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="relative mx-auto max-w-md px-6 sm:max-w-3xl lg:px-0">
                        <!-- Content area -->
                        <div class="pt-12 sm:pt-16 lg:pt-20">
                            <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Vernehmlassungen neu gedacht</h2>
                            <div class="mt-6 space-y-6 text-gray-500">
                                <p class="text-lg">Das Vernehmlassungsverfahren ist ein grundlegender, aber wenig bekannter Bestandteil der Schweizer Demokratie. Während es theoretisch allen offen steht, auch Einzelpersonen, machen fast nur organisierte Interessengruppen davon Gebrauch.</p>
                                <p class="text-base leading-7">Wir rücken Bürgerinnen und Bürger sowie kleinere Vereine ins Rampenlicht rücken und ihnen die Teilnahme an Vernehmlassungen und damit die Einflussnahme auf den Gesetzgebungsprozess auf Bundesebene erleichtern.</p>
                                <p class="text-base leading-7">Demokratis ist eine Webplattform für alle, die sich zu bestimmten politischen Themen Gehör verschaffen wollen. Wir setzen uns nicht nur dafür ein, den heutigen Prozess durch deliberative Zusammenarbeit zu erweitern, sondern möchten auch, dass Menschen direkt mit offiziellen Dokumenten arbeiten.</p>
                            </div>
                            <div class="mt-4">
                                <a href="#" class="text-base font-medium text-blue-500">Mehr über Vernehmlassungen erfahren &rarr;</a>
                            </div>

                        </div>


                    </div>
                </div>
            </div>

            <!-- Logo cloud section -->
            <div class="mt-32 2xl:mt-54">
                <div class="mx-auto max-w-md px-6 sm:max-w-3xl lg:max-w-7xl lg:px-8">
                    <div class="lg:grid lg:grid-cols-2 lg:items-center lg:gap-24">
                        <div class="md:-mt-40">
                            <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Blabla explore by Tag</h2>
                            <p class="mt-6 max-w-3xl text-lg leading-7 text-gray-500">Sagittis scelerisque nulla cursus in enim consectetur quam. Dictum urna sed consectetur neque tristique pellentesque. Blandit amet, sed aenean erat arcu morbi. Cursus faucibus nunc nisl netus morbi vel porttitor vitae ut. Amet vitae fames senectus vitae.</p>
                            <div class="mt-6">
                                <a href="#" class="text-base font-medium text-blue-500 hover:underline">Zur Übersicht aller Vernehmlassungen &rarr;</a>
                            </div>
                        </div>
                        <div class="grid grid-cols-1 gap-4 sm:grid-cols-2 md:grid-cols-4">
                            {% for tag in tags %}
                                <div class="relative flex items-center space-x-3 rounded-lg border border-gray-300 bg-white py-2 shadow-sm focus-within:ring-2 focus-within:ring-blue-500 focus-within:ring-offset-2 hover:border-gray-400">
                                    <a href="{{ path('app_consultation', { 't': tag.slug }) }}" class="after:absolute after:inset-0 mx-auto focus:outline-none truncate text-sm text-gray-500">
                                        {{ tag.name }}
                                    </a>
                                </div>
                            {% endfor %}
                        </div>
                        <!-- Stats section -->
                        <div class="2xl:-mt-40">
                            <dl class="grid grid-cols-2 gap-x-4 gap-y-8">
                                <div class="border-t-2 border-gray-100 pt-6">
                                    <dt class="text-base font-medium text-gray-500"># Vernehmlassungen</dt>
                                    <dd class="text-3xl font-bold tracking-tight text-gray-900">{{ count }}</dd>
                                </div>

                                <div class="border-t-2 border-gray-100 pt-6">
                                    <dt class="text-base font-medium text-gray-500">Aktuell laufen</dt>
                                    <dd class="text-3xl font-bold tracking-tight text-gray-900">{{ ongoing }}</dd>
                                </div>

                                <div class="border-t-2 border-gray-100 pt-6">
                                    <dt class="text-base font-medium text-gray-500">Geplant sind</dt>
                                    <dd class="text-3xl font-bold tracking-tight text-gray-900">{{ planned }}</dd>
                                </div>

                                {#
                                <div class="border-t-2 border-gray-100 pt-6">
                                    <dt class="text-base font-medium text-gray-500">Raised</dt>
                                    <dd class="text-3xl font-bold tracking-tight text-gray-900">$25M</dd>
                                </div>
                                #}
                            </dl>
                        </div>
                    </div>
                </div>
            </div>

            <!-- CTA section -->
            <div class="relative mt-24 sm:mt-32 sm:py-16">
                <div aria-hidden="true" class="hidden sm:block">
                    <div class="absolute inset-y-0 left-0 w-1/2 rounded-r-3xl bg-gray-50"></div>
                    <svg class="absolute top-8 left-1/2 -ml-3" width="404" height="392" fill="none" viewBox="0 0 404 392">
                        <defs>
                            <pattern id="8228f071-bcee-4ec8-905a-2a059a2cc4fb" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                                <rect x="0" y="0" width="4" height="4" class="text-gray-200" fill="currentColor" />
                            </pattern>
                        </defs>
                        <rect width="404" height="392" fill="url(#8228f071-bcee-4ec8-905a-2a059a2cc4fb)" />
                    </svg>
                </div>
                <div class="mx-auto max-w-md px-6 sm:max-w-3xl lg:max-w-7xl lg:px-8">
                    <div class="relative overflow-hidden rounded-2xl bg-blue-500 px-6 py-10 shadow-xl sm:px-12 sm:py-20">
                        <div aria-hidden="true" class="absolute inset-0 -mt-72 sm:-mt-32 md:mt-0">
                            <svg class="absolute inset-0 h-full w-full" preserveAspectRatio="xMidYMid slice" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 1463 360">
                                <path class="text-blue-400 text-opacity-40" fill="currentColor" d="M-82.673 72l1761.849 472.086-134.327 501.315-1761.85-472.086z" />
                                <path class="text-blue-600 text-opacity-40" fill="currentColor" d="M-217.088 544.086L1544.761 72l134.327 501.316-1761.849 472.086z" />
                            </svg>
                        </div>
                        <div class="relative">
                            <div class="sm:text-center">
                                <h2 class="text-3xl font-bold tracking-tight text-white sm:text-4xl">Einladung bekommen</h2>
                                <p class="mx-auto mt-6 max-w-2xl text-lg text-blue-100">Aktuell befinden wir uns in einer geschlossenen Testphase. Kontaktiert uns gerne, um einen Testzugang zu erhalten</p>
                            </div>

                            <form action="#" class="mt-12 sm:mx-auto sm:flex sm:max-w-lg">
                                <div class="min-w-0 flex-1">
                                    <label for="cta-email" class="sr-only">Email address</label>
                                    <input id="cta-email" type="email" class="block w-full rounded-md border border-transparent px-5 py-3 text-base text-gray-900 placeholder-gray-500 shadow-sm focus:border-transparent focus:outline-none focus:ring-2 focus:ring-white focus:ring-offset-2 focus:ring-offset-blue-500" placeholder="E-Mail">
                                </div>
                                <div class="mt-4 sm:mt-0 sm:ml-3">
                                    <button type="submit" class="block w-full rounded-md border border-transparent bg-gray-900 px-5 py-3 text-base font-medium text-white shadow hover:bg-black focus:outline-none focus:ring-2 focus:ring-white focus:ring-offset-2 focus:ring-offset-blue-500 sm:px-10">Testzugang anfragen</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </main>

        <!-- Footer section -->
        <footer class="mt-24 bg-gray-900 sm:mt-12">
            <div class="mx-auto max-w-md overflow-hidden py-12 px-6 sm:max-w-3xl lg:max-w-7xl lg:px-8">
                <nav class="-mx-5 -my-2 flex flex-wrap justify-center" aria-label="Footer">
                    <div class="px-5 py-2">
                        <a href="#" class="text-base text-gray-400 hover:text-gray-300">Verein</a>
                    </div>

                    <div class="px-5 py-2">
                        <a href="#" class="text-base text-gray-400 hover:text-gray-300">Kontakt</a>
                    </div>

                    <div class="px-5 py-2">
                        <a href="#" class="text-base text-gray-400 hover:text-gray-300">Impressum</a>
                    </div>
                </nav>
                {#
                <div class="mt-8 flex justify-center space-x-6">
